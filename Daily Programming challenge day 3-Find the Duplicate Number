#include <iostream>
using namespace std;

int findDuplicate(int arr[], int n) {
    int slow = arr[0];
    int fast = arr[0];

    do {
        slow = arr[slow];
        fast = arr[arr[fast]];
    } while (slow != fast);

    slow = arr[0];
    while (slow != fast) {
        slow = arr[slow];
        fast = arr[fast];
    }
    return slow;
}

int main() {
    int n;
    cout << "Enter n : ";
    cin >> n;

    int arr[n+1];
    cout << "Enter " << n+1 << " numbers between 1 and " << n <<endl;
    for (int i = 0; i < n+1; i++) {
        cin >> arr[i];
    }

    cout << "Duplicate number: " << findDuplicate(arr, n) << endl;
    return 0;
}
