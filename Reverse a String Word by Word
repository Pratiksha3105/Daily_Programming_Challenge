#include <iostream>
#include <sstream>
using namespace std;

int main() {
    string s;
    cout << "Enter a string: ";
    getline(cin, s); 

    stringstream ss(s); 
    string word;
    string words[100];  
    int count = 0;      

    while (ss >> word) {
        words[count] = word;
        count++;
    }

    for (int i = count - 1; i >= 0; i--) {
        cout << words[i];
        if (i > 0) cout << " "; 
    }

    return 0;
}
