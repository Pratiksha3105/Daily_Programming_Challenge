#include <iostream>
using namespace std;

int main() {
    int n;
    cout<<"Enter size of array: ";
    cin >> n;
    int arr[1000000]; 
    cout<<"Enter the numbers:";
    for(int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    int leaders[1000000];  
    int count = 0;

    int maxRight = arr[n-1];
    leaders[count++] = maxRight;

    for(int i = n-2; i >= 0; i--) {
        if(arr[i] > maxRight) {
            maxRight = arr[i];
            leaders[count++] = arr[i];
        }
    }
    for(int i = count-1; i >= 0; i--) {
        cout << leaders[i] << " ";
    }
    return 0;
}
